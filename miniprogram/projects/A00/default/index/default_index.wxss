@import "../../../../style/project/base/appt.wxss";
@import "../../skin/skin.wxss";
@import "../../../../style/project/default_index_base.wxss";

page {
	background-color: #333;
}

.up,
.up image {
	width: 100%;
	height: 650rpx;
}

.menu .item {
	width: 25%;
	height: 180rpx;
}

.pic-card {
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}

.pic-card image {
	width: inherit;
	height: inherit;
	border-radius: 10rpx;
	border: 1rpx solid rgba(171, 143, 114, 0.9)
}

.pic-card .title {
	z-index: 99999;
	color: #fff;
	font-weight: bold;
	top: 5rpx;
	left: 10rpx;
	position: absolute;
	font-size: 28rpx;
}

.pic-card .left {
	height: 500rpx;
	width: 320rpx;
	background-color: #fff;
	margin-right: 14rpx;
	border-radius: 10rpx;
	position: relative;
}

.pic-card .right {
	height: 500rpx;
	flex: 1;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.pic-card .right .item1,
.pic-card .right .item2 {
	width: 100%;
	height: 243rpx;
	border-radius: 10rpx;
	background-color: #fff;
	position: relative;
}

.pic-card .right .item1 {
	margin-bottom: 14rpx;
}

.company {
	width:100%;
	text-align: center;
	font-size:45rpx;
	font-weight: bold;
	color:var(--projectColor);
	margin-top:100rpx;
}

.btn-base {
	width: 220rpx;
	margin-top: 30rpx;
	margin-bottom:150rpx;
	height:80rpx;
	line-height: 80rpx;
}